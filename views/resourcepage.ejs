<div class="container">
  <div class="row">
    <div class="col s12 m12 l12 blue-grey-text text-darken-4">
      <h1>Resources</h1>
    </div>
  </div>
  <div class="row">
<ul>
    <% resources.forEach( resource => { %>
      <li>
      <div class="col s12 m6 l6">
        <div class="card amber darken-2 blue-grey-text text-darken-4">
          <div class="card-content">
            <span class="card-title"><p><%= resource.name_of_program %></p></span>
              <p><%= resource.location %></p>
              <p><%= resource.meal_served %></p>
          </div>
        </div>
      </div>
    </li>
    <% }) %>
  </ul>
  </div>
</div>
