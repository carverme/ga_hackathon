<div class="container">
  <div class="row">
    <div class="col s12 m12 l12 blue-grey-text text-darken-4">
      <h1>Requests</h1>
    </div>
  </div>
  <div class="row">
    <ul>
      <% requests.forEach( requests => { %>
        <li>
            <div class="col s12 m6">
              <div class="card amber darken-2">
                <div class="card-content">
                  <span class="card-title blue-grey-text text-darken-4"><%= requests.name %></span>
                  <p class="blue-grey-text text-darken-4"><%= requests.details %></p>
                </div>
                <div class="card-action">
                  <a class="blue-grey-text text-darken-4"href="#">Message</a>
                </div>
              </div>
            </div>
          </li>
        <% }) %>
      </ul>
    </div>
</div>
